							"mode": "raw",
							"raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"password123\",\n  \"confirm_password\": \"password123\",\n  \"first_name\": \"John\",\n  \"last_name\": \"Doe\",\n  \"gender\": \"male\",\n  \"birthday\": \"1990-01-15\",\n  \"street\": \"123 Main St\",\n  \"city\": \"New York\",\n  \"height\": 175,\n  \"weight\": 70,\n  \"shoulder_circumference\": 110,\n  \"arm_length\": 60,\n  \"breast_circumference\": 95,\n  \"under_breast_circumference\": 85,\n  \"waist_circumference\": 80,\n  \"hip_circumference\": 95,\n  \"upper_thigh_circumference\": 55,\n  \"neck_circumference\": 38,\n  \"biceps_circumference\": 30,\n  \"upper_hip_circumference\": 90,\n  \"wide_hip_circumference\": 100,\n  \"calf_circumference\": 35,\n  \"waist_to_crotch_front_length\": 25,\n  \"waist_to_crotch_back_length\": 30,\n  \"inner_leg_length\": 75,\n  \"foot_length\": 26,\n  \"foot_width\": 10,\n  \"unit\": \"metric\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/create-account",
							"host": ["{{base_url}}"],
							"path": ["api", "create-account"]
						}
					},
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 201) {",
									"    var jsonData = pm.response.json();",
									"    if (jsonData.data && jsonData.data.token) {",
									"        pm.collectionVariables.set('jwt_token', jsonData.data.token);",
									"        pm.collectionVariables.set('user_id', jsonData.data.user.userid);",
									"    }",
									"}"
								]
							}
						}
					]
				},
